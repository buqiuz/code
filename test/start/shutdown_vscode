#!/bin/bash

# 获取 VS Code 服务器的 PID
PIDS=$(ps -ef | grep vscode | grep -v grep | awk '{print $2}')

# 终止所有 VS Code 服务器进程
if [ ! -z "$PIDS" ]; then
  echo "Terminating VS Code server processes..."
  kill -9 $PIDS
  echo "VS Code server processes terminated."
else
  echo "No VS Code server processes found."
fi
